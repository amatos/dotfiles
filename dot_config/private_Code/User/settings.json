{
  "editor.fontFamily": "SF Mono, Operator Mono, Menlo, Monaco, 'Courier New', monospace",
    "editor.letterSpacing": 0.5,
    "files.trimTrailingWhitespace": true,
    "editor.fontWeight": "400",
    "prettier.eslintIntegration": true,
    "editor.cursorStyle": "line-thin",
    "editor.cursorWidth": 5,
    "editor.cursorBlinking": "solid",
    "editor.renderWhitespace": "all",
    "workbench.iconTheme": "vscode-icons",
    "git.enableCommitSigning": true,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.startupEditor": "none",
    "editor.fontLigatures": true,
    "files.autoGuessEncoding": true,
    "files.insertFinalNewline": true,
    "window.autoDetectColorScheme": true,
    "scm.inputFontSize": 14,
    "editor.scrollbar.horizontalScrollbarSize": 14,
    "cmake.showOptionsMovedNotification": false,
    "workbench.colorTheme": "Default Light Modern",
    "workbench.preferredDarkColorTheme": "Wal",
    "window.newWindowProfile": "Default",
    "workbench.preferredLightColorTheme": "Catppuccin Mocha",
  }
